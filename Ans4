n1=int(input('Enter the First no. :'))
n2=int(input('Enter the Second no. :'))
n3=int(input('Enter the Third no. :'))
if (n1>n2) and (n2>n3):
    print('Greatest:',n1)
elif(n2>n1) and (n2>n3):
    print('Greatest:',n2)
else :
    print('Greatest:',n3)
